{
    "Description": "Build a static PyQtWebEngine.",

    "qt5": {
        "linux-64#qt_dir":      "gcc_64",
        "macos-64#qt_dir":      "clang_64",
        "win-32#qt_dir":        "msvc2015",
        "win-64#qt_dir":        "msvc2017_64"
    },

    "python": {
        "build_host_from_source":   false,
        "build_target_from_source": true,
        "source":                   "Python-3.7.*.tar.xz"
    },

    "sip": {
        "module_name":  "PyQt5.sip",
        "source":       "sip-4.*.tar.gz"
    },

    "pyqt5": {
        "modules":  ["QtCore", "QtGui"],
        "source":   "PyQt5_*-5.*.tar.gz"
    },

    "pyqtwebengine": {
        "source":   "PyQtWebEngine_*-5.*.tar.gz"
    }
}
